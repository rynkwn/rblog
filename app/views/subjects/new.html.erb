<h1 class="secondary">New Subject</h1>

<%= form_for(@subject, html: {class: 'form', :autocomplete => "off"})do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  
  <%= f.submit "Create Subject", class: "btn-primary" %>
<% end %>
